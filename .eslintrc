{
  "extends": [
    "airbnb"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "react/jsx-no-bind": [0],
    "react/display-name": [0],
    "react/forbid-prop-types": [0]
  }
}
